extends skeleton
block title
	title Hello, World!
block content
	h1
		span Hello,&nbsp;
		span#name World!
	div.auth
		p Please type the following into your IRC client:
		div.prompt
			span &#47;msg&nbsp;
			em #{ bot.nick }&nbsp;
			span VERIFY&nbsp;
			em#slug #{ slug }
		p#status
			- Loading
			noscript (js disabled)
	div.privacy
		p
			a(href='/privacy') Privacy policy
block footer
	script(src='auth.js')
